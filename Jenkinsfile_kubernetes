pipeline {
    agent any
    stages {
        stage ('build') {
            steps {
                docker.withRegistry('https://registry.hub.docker.com', 'DOCKER_HUB') {
                    def customImage = docker.build('ilhamv/dockerwebapp')
                    customImage.push()
                }
                echo "Build App"
                echo "$BUILD_NUMBER"
                sleep (5)
            }
        }
    }
}
